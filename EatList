#!/bin/bash
 
# 定义要抽取的数据列表
data_list=(
"辣可可"
"萨莉亚"
"太二酸菜鱼"
"南京大牌档"
"蛙来哒"
"猪肚鸡"
"砂锅粥"
"羊肉煲"
"牛杂煲"
"海底捞"
"重庆火锅"
"猪肉串"
"楼下汤米粉-炒菜"
"沙县小吃"
"烧鸭叉烧饭"
"麻辣烫"
"隔壁村鸭货"
"隔壁村砂锅麻辣烫"
"喜姐炸串"
"梅县盐焗鸡"
"麦当劳"
"肯德基"
"木屋烧烤"
"楼下螺蛳粉"
"早茶"
"A区遇见小面-有余额"
"C区重庆小面"
"外卖-禄洺炒粉"
"外卖-禄洺茶点"
"外卖-潮客炒粉肉丸汤"
"外卖-苦瓜鸡"
"外卖-螺蛳粉"
"外卖-老盒饭"
"外卖-鸡公煲"
"外卖-盛发大排档-炒牛河-粉丝扇贝"
"外卖-拌将"
"外卖-鱼拿酸菜鱼"
)
 
# 生成一个0到4之间的随机索引值（包括边界）
random_index=$((RANDOM % ${#data_list[@]}))
 
# 获取对应索引位置上的元素
result=${data_list[$random_index]}

osascript -e 'tell app "System Events" to display dialog "'"$result"'"' 
osascript -e 'tell app "Terminal" to quit'

